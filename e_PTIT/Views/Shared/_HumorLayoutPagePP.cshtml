    <div class="container" style="margin-top:70px;background-color:#f9f7f4;color:#7e09c9;border-left:1px solid #808080;border-right:1px solid #808080;">
        <div style="margin-top:10px;"><h2 style="background-color:#7e09c9;color:#f9f7f4;border:1px solid #7e09c9;">Ե-ՓԹԻԹ 1</h2></div>
    </div>
    <div>
        @RenderBody()
    </div>


    <!-- Begin page content -->



    <div class="container-fluid" style=" padding: 0px !important;">
        <div class="footer">
            <div class="text-center">
                <img src="~/images/footer/newFooter.jpg" />
            </div>
            <div class="row" style="padding: 3px; color: #596982; background-color:white; height:50px;">
                <div class="col-md-3"></div>
                <div class="col-md-3">Copyright &copy 2015 PTIT. All rights reserved.</div>
                <div class="col-md-3" style="text-align: right;"><a href="~/Pages/ContactUs.aspx">Contact Us</a></div>
                <div class="col-md-3"></div>
            </div>
        </div>
    </div>
    <!-- jQuery -->
    <script src="~/Content/ptit/js/jquery.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Content/ptit/js/bootstrap.min.js"></script>
    <script src="~/Content/ptit/js/bootstrap-submenu.min.js"></script>
    <!-- Plugin JavaScript -->
    <script src="~/Content/ptit/js/jquery.easing.min.js"></script>
    <script src="~/Content/ptit/js/classie.js"></script>
    <script src="~/Content/ptit/js/cbpAnimatedHeader.js"></script>
