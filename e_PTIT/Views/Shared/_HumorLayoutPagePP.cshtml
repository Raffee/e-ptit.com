        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header page-scroll navbar-left" style="height:70px !important;">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand page-scroll" href="/Home/Index"><img src="~/images/ptitLettersLogo.jpg" style="width:80px" /></a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div id="bs-example-navbar-collapse-1" class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right" style="margin-top:10px;">
                    <li class="orange">
                        <div class="divOrange">
                            <span class="LeftSpanOrangeMenu"><img src="~/images/menuButton/orangeLeft.jpg" /></span>
                            <a href="/Home/About" class="OrangeMenuLink">Մեր Մասին</a>
                            <span class="RightSpanOrangeMenu"><img src="~/images/menuButton/orangeRight.jpg" /></span>
                        </div>
                    </li>
                    <li class="purple dropdown">
                        <div class="divPurple">
                            <span class="LeftSpanPurpleMenu"><img src="~/images/menuButton/purpleLeft.jpg" /></span>
                            <a href="#" class="PurpleMenuLink dropdown-toggle" data-toggle="dropdown">Յօդուածներ<span class="caret"></span></a>
                            <ul class="dropdown-menu" style="background-color: #f9f7f4 !important;">
                                <li><a href="/Articles/Index">ԽՄԲԱԳՐԱԿԱՆ</a></li>
                                <li><a href="/HealthArticles/Index">ԱՌՈՂՋԱՊԱՀԱԿԱՆ</a></li>
                            </ul>
                            <span class="RightSpanPurpleMenu"><img src="~/images/menuButton/purpleRight.jpg" /></span>
                        </div>
                    </li> <!-- .dropdown -->
                    <li class="blue">
                        <div class="divBlue">
                            <span class="LeftSpanBlueMenu"><img src="~/images/menuButton/blueLeft.jpg" /></span>
                            <a href="~/Games/LanguageGames.aspx" class="BlueMenuLink">Լեզուական Խաղեր</a>
                            <span class="RightSpanBlueMenu"><img src="~/images/menuButton/blueRight.jpg" /></span>
                        </div>
                    </li>
                    <li class="green">
                        <div class="divGreen">
                            <span class="LeftSpanGreenMenu"><img src="~/images/menuButton/greenLeft.jpg" /></span>
                            <a href="/Stories/Index" class="GreenMenuLink">Պատմութիւններ</a>
                            <span class="RightSpanGreenMenu"><img src="~/images/menuButton/greenRight.jpg" /></span>
                        </div>
                    </li>
                    <li class="red">
                        <div class="divRed">
                            <span class="LeftSpanRedMenu"><img src="~/images/menuButton/redLeft.jpg" /></span>
                            <a href="/Proverbs/Index" class="RedMenuLink">Առակներ</a>
                            <span class="RightSpanRedMenu"><img src="~/images/menuButton/redRight.jpg" /></span>
                        </div>
                    </li>
                    <li class="darkBlue">
                        <div class="divDarkBlue">
                            <span class="LeftSpanDarkBlueMenu"><img src="~/images/menuButton/darkBlueLeft.jpg" /></span>
                            <a href="#" class="DarkBlueMenuLink dropdown-toggle" data-toggle="dropdown">Այլ Խաղեր<span class="caret"></span></a>
                            <ul class="dropdown-menu" style="background-color: #f9f7f4 !important;">
                                <li><a href="~/Games/MathGamesList.aspx">ԹՈԻԱԲԱՆԱԿԱՆ ԽԱՂԵՐ</a></li>
                                <li><a href="~/Games/LogicalGamesList.aspx">ՏՐԱՄԱԲԱՆՈՒԹԵԱՆ ԽԱՂԵՐ</a></li>
                                <li><a href="~/Games/OtherGamesList.aspx">ԺԱՄԱՆՑԻ ԽԱՂԵՐ</a></li>
                            </ul>
                            <span class="RightSpanDarkBlueMenu"><img src="~/images/menuButton/darkBlueRight.jpg" /></span>
                        </div>
                    </li>
                    <li class="yellow">
                        <div class="divYellow">
                            <span class="LeftSpanYellowMenu"><img src="~/images/menuButton/yellowLeft.jpg" /></span>
                            <a href="/Riddles/Index" class="YellowMenuLink">Հանելուկներ</a>
                            <span class="RightSpanYellowMenu"><img src="~/images/menuButton/yellowRight.jpg" /></span>
